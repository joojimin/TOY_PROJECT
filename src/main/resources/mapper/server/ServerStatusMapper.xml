<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.toyproject.server.mapper.ServerStatusMapper">

    <select id="selectAll" resultType="com.my.toyproject.server.vo.ServerStatusVo">
        SELECT
            *
        FROM
            server_status_table
        WHERE
            openYn = 1
    </select>

</mapper>
